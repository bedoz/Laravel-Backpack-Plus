{
    "name": "bedoz/Laravel-Backpack-Plus",
    "type": "library",
    "description": "Extension of Laravel Backpack with lots of new features",
    "homepage": "https://github.com/bedoz/Laravel-Backpack-Plus",
    "license": "proprietary",
    "authors": [
        {
            "name": "Alex Bedendo",
            "email": "bedendoalex@gmail.com",
            "role": "Creator & Maintainer",
            "homepage": "https://alexbedendo.dev"
        }
    ],
    "require": {
        "laravel/framework": "^6.6.0",
        "backpack/crud": "^4.0.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "scrutinizer/ocular": "^1.6.0",
        "orchestra/testbench": "^4.4.1",
        "orchestra/database": "^4.0.1",
        "spatie/laravel-translatable": "^4.2.1",
        "astrotomic/laravel-translatable": "^11.6.1"
    },
    "autoload": {
        "psr-4": {
            "Bedoz\\BackpackPlus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bedoz\\BackpackPlus\\Tests\\": "tests"
        },
        "exclude-from-classmap": [
            "**/config"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bedoz\\BackpackPlus\\BackpackPlusServiceProvider"
            ],
            "aliases": {
                "CRUD": "Bedoz\\BackpackPlus\\app\\Library\\CrudPanel\\CrudPanelFacade"
            }
        }
    }
}
